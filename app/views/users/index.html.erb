<table>
<tr><th>Email</th><th>Admin?</th></tr>
<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.email, user_path(user) %></td>
    <td><%= user.admin? ? "yes" : "no" %></td>
  </tr>
<% end %>
</table>

